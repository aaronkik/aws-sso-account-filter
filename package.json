{
  "name": "aws-sso-account-filter",
  "version": "2023.5.29.1929",
  "author": "Aaron Kikabhai",
  "description": "A browser extension that reduces the list of AWS SSO accounts via custom regex filters",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "is-ci || husky install",
    "dev": "vite",
    "build:background": "vite build -c ./vite.background.config.ts",
    "build:actions": "vite build -c ./vite.actions.config.ts",
    "prebuild": "tsc --noEmit",
    "build": "npm run build:actions && npm run build:background",
    "local:clean:build": "rm -rf ./dist && npm run build",
    "lint": "prettier --check . && eslint . --ext .ts,.tsx",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "tailwind-merge": "^1.12.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chrome": "^0.0.237",
    "@types/node": "^18.16.14",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.29.8",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.29.8"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{ts,tsx}": "eslint --fix"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.6.7"
  }
}
