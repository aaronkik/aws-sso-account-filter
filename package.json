{
  "name": "aws-sso-account-filter",
  "version": "2025.8.10.1924",
  "author": "Aaron Kikabhai",
  "description": "A browser extension that reduces the list of AWS SSO accounts via custom regex filters",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "is-ci || husky",
    "dev": "vite",
    "build:background": "vite build -c ./vite.background.config.ts",
    "build:actions": "vite build -c ./vite.actions.config.ts",
    "prebuild": "tsc --noEmit",
    "build": "npm run build:actions && npm run build:background",
    "local:clean:build": "rm -rf ./dist && npm run build",
    "lint": "prettier --check . && eslint . --ext .ts,.tsx",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@fontsource/inter": "5.2.6",
    "@headlessui/react": "2.2.7",
    "@heroicons/react": "2.2.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "tailwind-merge": "3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/vite": "4.1.11",
    "@testing-library/jest-dom": "6.6.4",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/chrome": "0.1.3",
    "@types/node": "22.17.1",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "@vitejs/plugin-react": "5.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "10.4.21",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "4.6.2",
    "husky": "9.1.7",
    "is-ci": "4.1.0",
    "jsdom": "26.1.0",
    "lint-staged": "15.4.3",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "tailwindcss": "4.1.11",
    "typescript": "5.7.3",
    "vite": "7.1.1",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{ts,tsx}": "eslint"
  },
  "volta": {
    "node": "22.18.0",
    "npm": "11.5.2"
  }
}
