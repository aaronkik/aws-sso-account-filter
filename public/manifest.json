{
  "manifest_version": 3,
  "name": "AWS SSO Account Organiser",
  "version": "0.0.0",
  "action": {
    "default_popup": "actions/src/popup/index.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["background/index.js"],
      "matches": ["https://*.awsapps.com/*"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["storage"]
}
